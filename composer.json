{
    "name": "rommea/mail-tracker",
    "type": "library",
    "description": "Logs and tracks all outgoing emails from Laravel",
    "keywords": [
        "rommea",
        "MailTracker"
    ],
    "minimum-stability": "dev",
    "homepage": "https://github.com/rommeA/MailTracker",
    "license": "MIT",
    "authors": [
        {
            "name": "Anna Romme",
            "email": "anna.romme22@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~9.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "aws/aws-php-sns-message-validator": "^1.1",
        "aws/aws-sdk-php": "^3.133"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "~7.0",
        "mockery/mockery": "^1.4.4"
    },
    "suggest": {
        "fedeisas/laravel-mail-css-inliner": "Automatically inlines CSS into all outgoing mail."
    },
    "autoload": {
        "psr-4": {
            "rommeA\\MailTracker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rommeA\\MailTracker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "rommeA\\MailTracker\\MailTrackerServiceProvider"
            ]
        }
    }
}
